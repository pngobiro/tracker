
<table border ="1" cellpadding="1" cellspacing="5" >
<tr>
  <td><b>Case number:</b></td>
  <td><%= @casefile.casenumber %></td>
</tr>

<tr>
  <td><b>Date of filling:</b></td>
  <td><%= @casefile.dateoffilling %></td>
</tr>

<tr>
  <td><b>Court station:</b></td>
  <td><%= @casefile.courtstation.name %></td>
</tr>

<tr>
  <td><b>Deceased<b> </td>
  <td><%=@casefile.parties_names%> </td>
</tr>
<tr>
  <td><b>Deceased Citizenship<b> </td>
