</tr>
<tr>
<td><b>Remarks:</b></td>
<td> <%= @casefile.remarks %> </td>
</tr>

<tr>
<td><b>Mobile Number:</b></td>
<td> <%= @casefile.mobile_num %></td>
</tr>
</table>

<div id = "dashboardicons" >
<%= link_to link_to image_tag('/assets/edit.png') + 'Edit', edit_courtstation_casefile_path(@casefile.courtstation, @casefile) %>
<%= link_to link_to image_tag('/assets/back.jpeg') + 'Back',  courtstation_casefiles_path(@casefile.courtstation) %>
<%= link_to link_to image_tag('/assets/add.png') + 'New Movement', new_courtstation_casefile_movement_path(@casefile.courtstation, @casefile) %>
</div>

